---
import { Image } from "astro:assets";
import logoImage from "@/images/logo.webp";
import cn from "@/utils/cn";

const { class: className } = Astro.props;
---

<Image
  src={logoImage}
  width="150"
  height="150"
  alt="English for Abroad logo"
  loading="eager"
  class={cn("aspect-square", className)}
/>
