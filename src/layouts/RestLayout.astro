---
import FixedCTA from "@/components/FixedCTA.astro";
import Layout from "./Layout.astro";
import Navbar from "@/components/Navbar.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
  <Navbar slot="navbar" />

  <FixedCTA slot="fixed-cta" />

  <slot />
</Layout>
